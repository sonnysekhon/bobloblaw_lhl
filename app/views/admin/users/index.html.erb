<h1>Admin User Management Page</h1>
<%= paginate @users %>
<% @users.each do |user| %>
  <div class = "user">
  <%= user.firstname  %> | <%= user.lastname %> | <%= user.email %> | <%= link_to "edit" , edit_admin_user_path(user) %> | <%= link_to "delete", admin_user_path(user), method: :delete %> 

  </div>
<% end %>

<%= link_to "create user", new_admin_user_path %>