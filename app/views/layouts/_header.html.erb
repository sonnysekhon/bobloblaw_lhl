<header>
<nav class="teal lighten-1 navbar-fixed" role="navigation">
  <div class="nav-wrapper container">
    <div class="row">
      <div class="col s12 m3">
        <h4><a href="/" class="logo">
          <img class="logo-hover" src="<%= asset_path('book.png') %>">
        </a></h4>
      </div>
      <div class="col s12 m9">
        <div id="nav-mobile" class="right">
          <% if current_user %>
            <ul>
              <li><%= current_user.name %></li>
              <li class="avatar_thumbnail"><%= image_tag current_user.photo, alt: current_user.name %></li>
              <li><%= link_to 'Log Out', logout_path, method: :delete %></li>
            </ul>
          <% else %>
            <ul class="right-icons">
              <li>Login With</li>
              <li><%= link_to image_tag('twitter.png'), '/auth/twitter' %></li>
              <li><%= link_to image_tag('facebook.png'), '/auth/facebook' %></li>
            </ul>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</nav>
</header>
<br>