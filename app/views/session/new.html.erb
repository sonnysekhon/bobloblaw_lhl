<h2>log in</h2>
<% if current_user %>
  <p>already logged in</p>
<% else %>
<%= form_tag session_index_path do %>
  <div>
    <%= label_tag "email" %><br/>
    <%= text_field_tag "email" %>
  </div>
  <div>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div><%= submit_tag "Log in" %></div>
<% end %>
<% end %>
